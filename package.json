{
  "name": "rollup-plugin-stylus-import",
  "description": "A Rollup.js plugin to compile Stylus to css only",
  "version": "1.0.3",
  "author": "So What",
  "homepage": "https://github.com/robhicks/rollup-plugin-stylus-import",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohbicks/rollup-plugin-stylus.git"
  },
