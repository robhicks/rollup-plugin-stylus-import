    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.7.1",
